{
    "extends": "../tsconfig.json",
    "compilerOptions": {
        "target": "ES2022",
        "module": "CommonJS",  
        "outDir": "./dist",
        "rootDir": "./",
        "strict": true,
        "esModuleInterop": true,   
        "skipLibCheck": true,
        "moduleResolution": "node",
        "types": ["node"],    
        "sourceMap": true        
      },
    "include": ["**/*.ts"],
    "exclude": ["node_modules"]
  }